<div class="container col-12 mt-4">
<h3>Upload your E-Vehicle into the catalogue!</h3>
<%= simple_form_for(@product) do |f| %>
  <%= f.input :model %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :location %>
  <%= f.input :category %>
  <%= f.input :photo, as: :file %>
  <%= f.submit %>
<% end %>
</div>
